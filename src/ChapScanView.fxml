<?import javafx.scene.control.cell.PropertyValueFactory?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController">
    <children>
        <TabPane prefHeight="406.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Scan-In">
                    <content>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0">
                            <children>
                                <TableView fx:id="scanInTable" layoutY="54.0" prefHeight="319.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="54.0" />
                                <TextField fx:id="scanInText" layoutX="14.0" layoutY="14.0" onAction="#scanButtonPressed" prefHeight="27.0" prefWidth="437.0" promptText="Enter ID" AnchorPane.bottomAnchor="332.0" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0" />
                                <Button fx:id="scanInButton" layoutX="522.0" layoutY="14.0" mnemonicParsing="false" onAction="#scanButtonPressed" text="Scan In" AnchorPane.rightAnchor="14.0" />
                        <ToggleButton fx:id="lateToggleButton" layoutX="409.0" layoutY="14.0" mnemonicParsing="false" onAction="#toggleLate" text="Late" AnchorPane.rightAnchor="90.0" />
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Scan-Out">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                         <TableView fx:id="scanOutTable" layoutY="57.0" prefHeight="319.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="54.0" />
                         <TextField fx:id="scanOutText" layoutX="20.0" layoutY="14.0" onAction="#scanButtonPressed" prefHeight="27.0" prefWidth="480.0" promptText="Enter ID" AnchorPane.bottomAnchor="332.0" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0" />
                         <Button fx:id="scanOutButton" layoutX="509.0" layoutY="14.0" mnemonicParsing="false" onAction="#scanButtonPressed" text="Scan-Out" />
                     </children>
                  </AnchorPane>
               </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
